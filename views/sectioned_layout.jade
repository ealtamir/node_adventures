extends layout

block content
  div.main_div
    header#main_header
      block header
        div.header-content.pure-g-r
          div.pure-u-1-3
            img.header-logo(src="img/h_logo@2.png", alt="none")
          div.pure-u-1-3
          div.pure-u-1-3.links-container
            nav.header-links
              ul.f_large
                li.h_link Ingresar
                li.h_link.sep |
                li.h_link Registrarse
    section#main_section
      block main_section
        footer#main_footer
          block main_footer
            div.footer-content.pure-g-r
              div.pure-u-1-3
                p part1
              div.pure-u-1-3
                p part2
              div.pure-u-1-3
                p part3

  block js_scripts
    script(src='javascripts/lib/jquery.min.js')
    script(src='javascripts/lib/underscore-min.js')
    script(src='javascripts/lib/backbone-min.js')
    script(src='http://code.jquery.com/ui/1.10.3/jquery-ui.js')

