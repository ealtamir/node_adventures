extends layout

block content
  div.main_div
    header#main_header
      block header
        div.header-content.pure-g-r
          div.pure-u-1-3
            img.header-logo(src="", alt="none")
          div.pure-offset-1-3.pure-u-1-3
            nav.header-links
              ul
                li first link
                li second link
                li third link
    section#main_section
      block main_section
        footer#main_footer
          block main_footer
            div.footer-content.pure-g-r
              div.pure-u-1-3
                p part1
              div.pure-u-1-3
                p part2
              div.pure-u-1-3
                p part3

  block js_scripts
    script(src='javascripts/lib/jquery.min.js')
    script(src='javascripts/lib/underscore-min.js')
    script(src='javascripts/lib/backbone-min.js')
    script(src='http://code.jquery.com/ui/1.10.3/jquery-ui.js')

